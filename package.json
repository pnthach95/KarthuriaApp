    "intl-pluralrules": "^2.0.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.73.1",
    "react-native-android-widget": "^0.9.0",
    "react-native-bootsplash": "^5.1.4",
    "react-native-device-info": "^10.12.0",
